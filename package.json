{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "rm -rf node_modules bun.lock apps/*/node_modules packages/*/node_modules",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "turbo": "^2.8.3",
    "typescript": "5.9.3",
    "ultracite": "7.1.1"
  },
  "overrides": {
    "@sinclair/typebox": "0.34.48"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.3",
  "workspaces": {
    "catalog": {
      "elysia": "1.4.22",
      "drizzle-orm": "0.45.1"
    },
    "packages": [
      "apps/*",
      "packages/*"
    ]
  }
}
